type vendor_audionxp, domain;

type vendor_audionxp_exec, exec_type, vendor_file_type, file_type;

init_daemon_domain(vendor_audionxp)

allow vendor_audionxp proc_asound:dir search;
allow vendor_audionxp vendor_proc_audiod:file r_file_perms;
allow vendor_audionxp audio_device:dir search;
allow vendor_audionxp audio_device:chr_file rw_file_perms;
allow vendor_audionxp vendor_sysfs_kpi:file w_file_perms;
allow vendor_audionxp vendor_acdb_loader_service_service:service_manager find;
allow vendor_audionxp vendor_audio_hal_plugin_service_service:service_manager find;
binder_call(vendor_audionxp,vendor_audio_hal_plugin_service)
binder_call(vendor_audionxp,vendor_acdb_loader_service)
vndbinder_use(vendor_audionxp)
get_prop(vendor_audionxp, vendor_audio_prop)
